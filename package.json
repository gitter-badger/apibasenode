{
  "name": "boilerplate-api-express-node",
  "version": "1.0.0",
  "description": "Api OOD base",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node ./src/index.js",
    "prestartdev": "./node_modules/.bin/pm2 delete ecosystem.json; ",
    "startdev": "pm2 startOrReload ecosystem.json",
    "startdev:stop": "./node_modules/.bin/pm2 delete ecosystem.json",
    "startdev:kill": "./node_modules/.bin/pm2 kill",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:destroy": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seeder": "./node_modules/.bin/sequelize db:seed:all",
    "db:seeder:destroy": "./node_modules/.bin/sequelize db:seed:undo:all",
    "prebuild": "rm -rf ./dist",
    "build": "./node_modules/.bin/babel ./src --out-dir dist/",
    "prestartbuilded": "./node_modules/.bin/babel ./src --out-dir dist/",
    "startbuilded": "node ./dist/index.js",
    "debug": "babel-node ./src/index.js --nolazy --inspect-brk=9229",
    "pretest": "rm -f ./database/*.test.*; npm run build;cross-env NODE_ENV=test ./node_modules/.bin/sequelize db:migrate; cross-env NODE_ENV=test node_modules/.bin/sequelize db:seed:all",
    "test": "cross-env NODE_ENV=test mocha test --exit",
    "postest":"npm run clean:build",
    "cover": "cross-env NODE_ENV=test nyc mocha test --exit",
    "coverreport": "nyc report --reporter=lcov",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "coverserver": "nyc report --reporter=lcov; ./node_modules/.bin/browser-sync start --server \"coverage/lcov-report\"/",
    "apidoc": "apidoc -i ./src/http/routes -o ./docs",
    "preapidoc:server": "npm run apidoc",
    "apidoc:server": "./node_modules/.bin/browser-sync start --server ./docs",
    "clean": "rm -f package-lock.json; rm -rf node_modules",
    "clean:build": "rm -rf dist/",
    "lint": "eslint src/**/**/*.js src/**/*.js src/*.js  database/**/*.js test/**/**/*.js test/*.js",
    "lint:fix": "eslint src/**/**/*.js src/**/*.js src/*.js  database/**/*.js test/**/**/*.js test/*.js --fix"
  },
  "nyc": {
    "exclude": [
      "test",
      "dist",
      "database",
      "log",
      "public",
      "docs",
      "./src/domain",
      "./src/config",
      "./src/http/middlewares/security.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "bcrypt": "^2.0.0",
    "body-parser": "^1.18.2",
    "consign": "^0.1.6",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "express-winston": "^2.4.0",
    "moment": "^2.21.0",
    "passport": "^0.4.0",
    "passport-hash": "^0.6.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.11",
    "sqlite3": "^4.0.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "apidoc": "0.17.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-istanbul": "0.12.2",
    "babel-plugin-istanbul": "4.1.6",
    "babel-register": "6.26.0",
    "browser-sync": "2.23.6",
    "coveralls": "3.0.0",
    "cross-env": "5.1.4",
    "eslint": "4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "istanbul": "0.4.5",
    "mocha": "5.1.0",
    "nyc": "11.6.0",
    "pm2": "2.10.2",
    "sequelize-cli": "4.0.0",
    "supertest": "3.0.0"
  }
}
