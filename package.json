{
  "name": "boilerplate-api-express-node",
  "version": "1.0.0",
  "description": "A",
  "main": "index.js",
  "scripts": {
    "prestart": "./node_modules/.bin/sequelize db:migrate",
    "start": "set NODE_ENV=development;babel-node ./src/index.js",
    "pm2": "pm2 delete ecosystem.json; pm2 startOrReload ecosystem.json --interpreter babel-node",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "prebuild": "rm -rf ./dist",
    "build": "./node_modules/.bin/babel ./src --out-dir dist/",
    "debug": "babel-node ./src/index.js --nolazy --inspect-brk=9229",
    "pretest": "rm -f ./database/db.test.sqlite; NODE_ENV=test ./node_modules/.bin/sequelize db:migrate; NODE_ENV=test node_modules/.bin/sequelize db:seed:all",
    "test": "NODE_ENV=test mocha test --exit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "consign": "^0.1.6",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.3",
    "express-winston": "^2.4.0",
    "moment": "^2.21.0",
    "passport": "^0.4.0",
    "passport-hash": "^0.6.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.11",
    "sqlite3": "^3.1.13",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.0",
    "mocha": "^5.0.4",
    "sequelize-cli": "^3.1.0",
    "supertest": "^3.0.0"
  }
}
